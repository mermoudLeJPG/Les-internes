{
  "name": "les-internes-site",
  "version": "0.0.0",
  "description": "un site qui permet de telecharger tout nos programme dans un .exe fais avec pkg",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "pack": "pkg package.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "les internes",
  "license": "ISC",
  "dependencies": {
    "express": "^4.21.1",
    "node": "^20.2.0",
    "pkg": "^5.8.1"
  },
  "bin": "app.js",
  "pkg": {
    "scripts": "public/**/*.js",
    "assets": "public/**/*",
    "targets": [
      "node18"
    ],
    "outputPath": "dist"
  }
}